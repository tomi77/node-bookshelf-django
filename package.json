{
  "name": "bookshelf-django",
  "version": "0.1.2",
  "description": "Django models for Bookshelf",
  "main": "index.js",
  "scripts": {
    "prepare": "coffee --bare --compile --output . src/",
    "lint": "coffeelint src/",
    "test": "mocha --compilers coffee:coffee-script/register --require coffee-coverage/register-istanbul --recursive",
    "coverage": "npm test && istanbul report",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomi77/node-bookshelf-django.git"
  },
  "keywords": [
    "bookshelf",
    "django"
  ],
  "author": "Tomasz Jakub Rup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomi77/node-bookshelf-django/issues"
  },
  "homepage": "https://github.com/tomi77/node-bookshelf-django",
  "dependencies": {
    "bluebird": "^3.5.0"
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "coffee-coverage": "^2.0.1",
    "coffee-script": "^1.10.0",
    "coffeelint": "^1.15.7",
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.0",
    "sqlite3": "^3.1.4"
  },
  "peerDependencies": {
    "bookshelf": "^0.10.0"
  }
}
